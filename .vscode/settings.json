{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "public": true,
        "resources/_gen": true,
        "resources": true,
        ".vscode": true,
        ".gitignore": true,
        "go.mod": true,
        "go.sum": true,
        "netlify.toml": true,
        ".editorconfig": true
    },
    "hide-files.files": [
        "public",
        "resources/_gen",
        "resources",
        ".vscode",
        ".gitignore",
        "go.mod",
        "go.sum",
        "netlify.toml",
        ".editorconfig"
    ],
    "diffEditor.wordWrap": "off",
    "markdown.preview.breaks": true,
    "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
    "markdown-preview-enhanced.alwaysShowBacklinksInPreview": false,
    "markdown-preview-enhanced.codeBlockTheme": "github-dark.css",
    "markdown-preview-enhanced.enableLinkify": true,
    "markdown-preview-enhanced.enablePreviewZenMode": false,
    "markdown-preview-enhanced.hideDefaultVSCodeMarkdownPreviewButtons": true,
    "markdown-preview-enhanced.printBackground": true,
    "markdown-preview-enhanced.enableExtendedTableSyntax": true,
    "markdown-preview-enhanced.enableHTML5Embed": true,
    "markdown-preview-enhanced.enableScriptExecution": true,
    "markdown-preview-enhanced.enableTypographer": true,
    "markdown-preview-enhanced.frontMatterRenderingOption": "code block",
    "markdown-preview-enhanced.previewTheme": "atom-dark.css"
}